{
  "name": "expoapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "config": {
    "lingo_env": "--env-file ./config/.env.lingo",
    "campus_env": "--env-file ./config/.env.campus"
  },
  "scripts": {
    "web:campus": "expo start --web --port 6201",
    "web:lingo": "expo start --web --port 6201",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "build:lingo": "cross-env eas build --platform android --profile production $npm_package_config_lingo_env",
    "about:build:lingo": "generates .Aab which can be uploaded to Google Play Store",
    "build:campus": "eas build -p android --profile production",
    "build:lingo:dev": "cross-env eas build --platform android --profile development $npm_package_config_lingo_env",
    "about:build:lingo:dev": "generates .apk which can be installed on Android devices",
    "build:campus:dev": "cross-env eas build --platform android --profile development $npm_package_config_campus_env",
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "export-w": "npx expo export --platform web",
    "serve-w": "npx serve dist --single",
    "start-w": "yarn run export-w && yarn run serve-w",
    "eas-submit": "eas submit --platform android",
    "percy": "percy exec -- node percy-test.mjs",
    "ui-setup": "concurrently  \"npx expo install\" --names \"root,expoApp\" --success first || (echo \"Package installation failed!\"; exit 1) && yarn run web:campus"
  },
  "jest": {
    "preset": "jest-expo"
  },
